refuge-result {
  display: grid;
  grid-template-columns: 48px auto 120px 24px;
  height: 88px;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  padding: 0px;
  /* padding: 16px 24px 16px 16px; */
  /* Component styles go here */
}

.result-name {
  display: block;
  padding: 0;
  margin: 0;
}

.result-address {
  display: block;
  padding: 0;
  margin: 0;
  color: var(--dark-grey);
}

.marker {
  height: 24px;
  width: 18px;
}

.left {
  grid-column: 1;
  text-align: center;
}

.center {
  grid-column: 2;
  text-align: left;
  padding-right: 16px;
}

center::after {
  content: "";
  position: absolute;
  width: auto;
  height: 10px;
  left: 0;
  right: 0;
  background: var(--light-grey);
  bottom: 1px;
}

.right {
  grid-column: 3;
}