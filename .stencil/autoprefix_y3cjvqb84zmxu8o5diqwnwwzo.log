.result-name {
  display: block;
  padding: 0;
  margin: 0;
  width: 100%;
}

.result-address {
  display: block;
  padding: 0;
  margin: 0;
  width: 100%;
  color: var(--dark-grey);
}

.marker {
  height: 24px;
  width: 18px;
}

.result-content {
  padding-left: 16px;
  height: 100%;
}

.left {
  grid-column: 1;
  text-align: center;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}

.center {
  grid-column: 2;
  text-align: left;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}

.right {
  grid-column: 3;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}