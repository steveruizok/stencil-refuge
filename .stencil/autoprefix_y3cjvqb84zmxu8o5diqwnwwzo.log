.result-container {
  display: grid;
  grid-template-columns: 48px auto 120px;
  position: relative;
  -ms-flex-line-pack: center;
  align-content: center;
  padding: 16px 0;
  /* border-bottom: 1px solid var(--light-grey); */
}

