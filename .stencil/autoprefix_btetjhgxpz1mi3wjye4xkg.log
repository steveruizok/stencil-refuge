refuge-prediction {
  display: grid;
  grid-template-columns: 48px auto;
  /* padding: 16px 24px 16px 16px; */
  /* Component styles go here */
}

.prediction-container {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-line-pack: center;
  align-content: center;
  padding: 16px 24px 16px 16px;
  background: none;
}

.prediction-content {
  padding-left: 16px;
  height: 100%;
}

.prediction-content::after {
  content: "ok";
  position: absolute;
  width: auto;
  height: 1px;
  left: 48px;
  right: 24px;
  background: var(--dark-grey);
  bottom: 0px;
}

.prediction-left {
  grid-column: 1;
  text-align: center;
}