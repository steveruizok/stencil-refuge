  /* Component styles go here */
}

.prediction-container {
  display: grid;
  grid-template-columns: 48px auto;
  padding: 0 24px;
  background: none;
}

.prediction-left {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-line-pack: center;
  align-content: center;
  grid-column: 1;
  text-align: center;
}

.prediction-content {
  padding: 32px 0;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-line-pack: center;
  align-content: center;
  grid-column: 2;
  height: 100%;
  border-bottom: 1px solid var(--light-grey);
}