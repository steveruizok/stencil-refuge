refuge-results {
  grid-row: 5;
  overflow: scroll;
  position: relative;
  height: 0px;
  width: 100%;
  background: var(--white);
  -webkit-box-shadow: var(--shadow);
  box-shadow: var(--shadow);
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  -webkit-transition: height 0.25s;
  transition: height 0.25s;
}

.open {
  -webkit-transition: height 0.25s;
  transition: height 0.25s;
  height: 40vh;
}

ul {
  list-style-type: none;
  padding: 0;
  margin: 0;
}