refuge-prediction {
  display: grid;
  grid-template-columns: 48px -webkit-fill-available;
  grid-template-columns: 48px stretch;
  height: 88px;
  /* padding: 16px 24px 16px 16px; */
  /* Component styles go here */
}

.marker {
  grid-column: 1;
  height: 24px;
  width: 18px;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}

.prediction-content {
  grid-column: 2;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
}