.prediction-container {
  display: grid;
  grid-template-columns: 48px auto;
  padding: 16px 24px 16px 16px;
  background: none;
}

.prediction-left {
  grid-column: 1;
  text-align: center;
}

.prediction-content {
  grid-column: 2;
  height: 100%;
}