refuge-prediction {
  height: 88px;
  /* padding: 16px 24px 16px 16px; */
  /* Component styles go here */
}

.prediction-container {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  -ms-flex-line-pack: center;
  align-content: center;
  padding: 32px 24px 32px 16px;
  /* border-bottom: 1px solid var(--light-grey); */
}

.prediction-container::after {
  content: "";
  position: absolute;
  width: auto;
  height: 1px;
  left: 48px;
  right: 0px;
  background: var(--light-grey);
  bottom: 1px;
}

.marker {
  height: 24px;
  width: 18px;
}

.prediction-content {
  padding-left: 16px;
  height: 100%;
}