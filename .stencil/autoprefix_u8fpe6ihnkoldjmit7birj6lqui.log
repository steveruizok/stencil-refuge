refuge-prediction {
  display: grid;
  grid-template-columns: 48px auto;
  width: 100%;
  z-index: 100;
  margin: 0 0;
  height: 88px;
  /* padding: 16px 24px 16px 16px; */
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  /* Component styles go here */
}

.marker {
  grid-column: 1;
  -ms-flex-item-align: center;
  align-self: center;
  height: 24px;
  width: 18px;
}

.prediction-content {
  grid-column: 2;
  -ms-flex-item-align: center;
  align-self: center;
}